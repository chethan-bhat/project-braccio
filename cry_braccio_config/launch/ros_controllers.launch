<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find cry_braccio_config)/config/ros_controllers.yaml" command="load"/>


  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" ns="/braccio" output="screen" args="--namespace=/braccio joint_state_controller arm_controller --timeout 20"/>

</launch>
